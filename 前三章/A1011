#include<cstdio>
chansr S[3]={'W','T','L'};
int aansin(){
	
	double ans=1.0, tmp, a;
	int idx;
	for(int i=0;i<3;i++)
	{
		tmp=0.0;
		for(int j=0;j<3;j++)
		{
			scansnf("%1f",&a);
			if(a>tmp){
				tmp=a;
				idx=j;
						}	
		}
		printf("%c ",S[idx]);
		ans*=tmp;

	}
	printf("%.2f",(ans*0.65-1)*2);
	return 0;

} 